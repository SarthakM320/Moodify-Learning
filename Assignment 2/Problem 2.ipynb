    "def sigmoid(x):\n",
    "    return 1/(1+np.exp(-x))\n",
    "\n",
    "def net_input(x, theta):\n",
    "    return np.dot(x, theta)\n",
    "\n",
    "def probability(theta, x):\n",
    "    return sigmoid(net_input(x, theta))\n",
    "\n",
    "(net_input(x, theta)-y).shape"
   ]
  },
  {
