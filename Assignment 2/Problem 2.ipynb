    "x = np.concatenate([np.ones((x_train.shape[0], 1)), x_train], axis = 1)\n",
    "theta = np.ones([x.shape[1],1])\n",
    "y = y_train.reshape(4000,1)\n",
    "x_test = np.concatenate([np.ones((x_test.shape[0], 1)), x_test], axis = 1)"
   ]
